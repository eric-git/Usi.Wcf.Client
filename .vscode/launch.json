{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/Console/bin/Debug/net8.0/Console.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/Console",
            "launchSettingsFilePath": "${workspaceFolder}/src/Console/Properties/launchSettings.json",
            "launchSettingsProfile": "2.1 IssuerBinding Mode (VA1803)",
            "console": "externalTerminal",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}
